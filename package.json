{
  "name": "geoffrothman",
  "description": "commands from geoff rothman",
  "version": "1.0",
  "author": "Geoff Rothman @geoffrothman",
  "bugs": "https://github.com/geoffrothman/gplugin/issues",
  "dependencies": {},
  "devDependencies": {},
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/lib",
    "/messages"
  ],
  "homepage": "https://github.com/geoffrothman/gplugin",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "geoffrothman/gplugin",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "clean": "rm -f .oclif.manifest.json",
    "lint": "tsc -p test --noEmit && tslint -p test -t stylish",
    "postpublish": "yarn run clean",
    "posttest": "yarn run lint",
    "prepare": "yarn run build && oclif-dev manifest",
    "prepublishOnly": "yarn run build && oclif-dev manifest",
    "preversion": "yarn run clean",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
